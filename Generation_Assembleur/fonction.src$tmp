RESETA 	equ 0xEFFA 	// adresse de lancement
INA    	equ 0xEFA0 	// adresse du port // d'entree
OUTA   	equ 0xEFA2 	// adresse du port // de sortie
LOADA  	equ 0xEFFA      // adresse de chargement
SEUIL	equ 3		// seuil
ORG   LOADA	// chargement en LOADA
START RESETA	// demarre en RESETA
LDW R3, #7666
STW R3, -(R15)
LDW R0, R15
LDQ 0, R3
STW R3, -(R15)
JSR @printi
STRING0 string "COUCOU1"
LDW R3, #STRING0
LDW R0, R3
JSR @print
STRING1 string "COUCOU2"
LDW R3, #STRING1
LDW R0, R3
JSR @print
LDQ 64, R14 //On charge la trappe EXIT 
TRP R14 //On quitte l'exicution
print LDQ 66, R14
TRP R14 //On lhve la trappe
RTS //On retourne dans la suite de l'exicution
printi LDQ 66, R14
TRP R14 //On hve la trappe
RTS //On retourne dans la suite de l'exicution
read LDQ 65, R14 //on charge la trap
LDQ 0, R0 //On ricuphre les chaines de caracthres tout le temps au mjme endroit, en dibut de mimoire
TRP R14 //On lhve la trappe
RTS //On retourne dans le programme
