* /
+ -
<> < <= > >= =
&   
|
:= 

A	:	C A2
	;

A2	:	'|' C A2
	|;


B	:	C B2
	;

B2	:	'&' C B2
	|;

C	:	E C2
	;

C2	:	'<' C3
	|	'>' C4
	|	'=' E C2
	|;

C3	:	'>' E C2
	|	'=' E C3
	|	E C2
	;

C4	:	E C2
	|	'=' E C2
	;	

E	:	T E'
	;

E'	:	'+' T E'	
	|	'-' T E'
	|;

T	:	F T'
	;

T'	: 	'*' F T'
	|	'/' F T'
	|;

F	:	'('A')'
	|	int	
	|	id
	;
